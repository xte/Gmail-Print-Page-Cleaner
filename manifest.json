{
  "manifest_version": 2,

  "name": "Gmail Print Page Cleaner",
  "version": "1.0.1",
  "description": "Removes everything but the body of the email from the Gmail print email page.",

  "permissions": [
    "https://mail.google.com/"
  ],

  "web_accessible_resources": ["jquery-2.0.3.min.js"],

  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["jquery-2.0.3.min.js", "main.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
